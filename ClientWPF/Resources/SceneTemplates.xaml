<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:scenes="clr-namespace:ClientWPF.Scenes.StartScreen"
                    xmlns:room="clr-namespace:ClientWPF.Scenes.RoomScene"
                    xmlns:local="clr-namespace:ClientWPF.Resources"
                    x:Name="SceneTemplatesDict">
    <DataTemplate DataType="{x:Type scenes:StartScreenVm}" x:Key="T1">
        <scenes:StartControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type room:RoomVm}">
        <room:RoomControl/>
    </DataTemplate>


</ResourceDictionary>