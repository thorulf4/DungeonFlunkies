<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:scenes="clr-namespace:ClientWPF.Scenes.StartScreen"
                    xmlns:room="clr-namespace:ClientWPF.Scenes.RoomScene"
                    xmlns:char="clr-namespace:ClientWPF.Scenes.Character"
                    xmlns:combat="clr-namespace:ClientWPF.Scenes.Combat"
                    x:Name="SceneTemplatesDict">
    <DataTemplate DataType="{x:Type scenes:StartScreenVm}">
        <scenes:StartControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type room:RoomVm}">
        <room:RoomControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type char:InventoryVm}">
        <char:InventoryControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type combat:ChooseVm}">
        <combat:ChooseControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type combat:ChooseTargetVm}">
        <combat:ChooseTargetControl/>
    </DataTemplate>


</ResourceDictionary>